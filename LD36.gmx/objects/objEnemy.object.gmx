            <string>///Move until a certain spot

if (instance_number(objGameOverReport) &lt;= 0) {

  var colliding = place_meeting(x + moveSpeedX, y + moveSpeedY, objEnemy);
  collidingWith = noone;
  
  if (colliding) {
    collidingWith = instance_place(x + moveSpeedX, y + moveSpeedY, objEnemy);
  }
  
  if (collidingWith != noone) {
    with (collidingWith) {
      if (collidingWith == other.id) {
        colliding = false;
      }
    }
  }
  
  if (moveSpeedX &lt; 0) {
    if (x &gt; moveEndX &amp;&amp; !colliding) {
      x += moveSpeedX;
    }
  } else {
    if (x &lt; moveEndX &amp;&amp; !colliding) {
     x += moveSpeedX;
    }
  }
  
  if (moveSpeedY &lt; 0) {
    if (y &gt; moveEndY &amp;&amp; !colliding) {
      y += moveSpeedY;
    }
  } else {
    if (y &lt; moveEndY &amp;&amp; !colliding) {
      y += moveSpeedY;
    }
  }
}
</string>
